{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api.py"
        }
    ]
}